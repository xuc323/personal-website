{
  "$schema": "https://raw.githubusercontent.com/firebase/firebase-tools/master/schema/firebase-config.json",
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "frameworksBackend": {
      "concurrency": 80,
      "enforceAppCheck": true,
      "maxInstances": 6,
      "memory": "256MiB",
      "minInstances": 0,
      "region": "us-central1",
      "timeoutSeconds": 60
    }
  }
}
